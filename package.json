{
  "name": "server_v4",
  "version": "1.0.0",
  "description": "School Admin API Server",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "start:prod": "NODE_ENV=production node src/server.js",
    "dev": "NODE_ENV=development nodemon src/server.js",
    "dev:debug": "NODE_ENV=development nodemon --inspect src/server.js",
    "test": "NODE_ENV=test jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "clean": "rm -rf logs/* uploads/*",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "boxen": "^8.0.1",
    "chalk": "^4.1.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.3",
    "express-validator": "^7.2.1",
    "figlet": "^1.7.0",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.2",
    "ora": "^5.4.1",
    "redis": "^5.0.1",
    "sequelize": "^6.37.1",
    "strip-ansi": "^6.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/node": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.3"
  }
}
